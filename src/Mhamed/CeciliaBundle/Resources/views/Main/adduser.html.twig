<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
	<link rel="icon" href="{{asset('bundles/mhamedCecilia/images/logo.jpg')}}">

<link href="{{asset('bundles/mhamedCecilia/css/family=Kaushan+Script.css')}}" rel='stylesheet' type='text/css'>
    <link href="{{asset('bundles/mhamedCecilia/css/family=Montserrat.css')}}" rel="stylesheet" type="text/css">

    <link rel="stylesheet" href="{{asset('bundles/mhamedCecilia/css/bootstrap.min.css')}}">

	

    <title>Dr Cecilia</title>


    <!-- Custom CSS -->
    <link href="{{asset('bundles/mhamedCecilia/css/homepage stylesheet.css')}}" rel="stylesheet">
	<link href="{{asset('bundles/mhamedCecilia/css/agency.css')}}" rel="stylesheet">
    <style>
    body{
    	padding-top: 50px;
    }
    </style>

</head>
<body>
	



	<!-- Section contact -->
	<section id="contact">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Contact Us</h2>
                    <h3 class="section-subheading text-muted">                                       </h3>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    {{ form_start(form, {'attr': {'id': 'contactForm'}}) }}
                        <div class="row">
                            <div class="col-md-6">
                                <div class="form-group">
                                	{{ form_errors(form.name) }}
                                	{{ form_widget(form.name) }}
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                	{{ form_errors(form.email) }}
                                	{{ form_widget(form.email) }}
                                    <p class="help-block text-danger"></p>
                                </div>
                                <div class="form-group">
                                	{{ form_errors(form.phone) }}
                                	{{ form_widget(form.phone) }}
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="col-md-6">
                                <div class="form-group">
                                	{{ form_errors(form.message) }}
                                	{{ form_widget(form.message) }}
                                    <p class="help-block text-danger"></p>
                                </div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="col-lg-12 text-center">
                                <div id="success"></div>
                                {{ form_widget(form.Send) }}
                            </div>
                        </div>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </section>
    <img src="{{asset('bundles/mhamedCecilia/images/back1.jpg')}}"/>

{% if form.vars.valid %}
{{ render(controller('MhamedCeciliaBundle:Main:modal')) }} 
{% endif %}





</body>
</html>